{
    "name": "环保降解袋",
    "description": "环保降解袋",
    "keywords": ["环保降解袋"],
    "type": "project",
    "authors": [
        {
            "name": "btan",
            "email": "tyb0214@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "laravel/passport": "1.0.*",
        "predis/predis": "^1.1",
        "laravelcollective/html": "^5.3",
        "yajra/laravel-datatables-oracle": "^6.17",
        "guzzlehttp/guzzle": "^6.2",
        "zizaco/entrust": "5.2.x-dev",
        "hieu-le/active": "^3.3",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "toplan/laravel-sms": "2.5.*",
        "germey/geetest": "^1.0",
        "dingo/api": "1.0.x@dev",
        "baum/baum": "^1.1",
        "stevenyangecho/laravel-u-editor": "^1.3",
        "talvbansal/media-manager": "^1.1.*",
        "doctrine/dbal": "^2.5",
        "intervention/image": "^2.3",
        "intervention/imagecache": "^2.3",
        "proengsoft/laravel-jsvalidation": "1.3@dev",
        "jildertmiedema/laravel-plupload": "^0.3.1",
        "arcanedev/log-viewer": "^4.0",
        "laravel/scout": "^1.1",
        "elasticsearch/elasticsearch": "^5.0",
        "jpush/jpush": "^3.5"
        
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-debugbar": "^2.2",
        "barryvdh/laravel-ide-helper": "^2.2",
        "lsrur/inspector": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
